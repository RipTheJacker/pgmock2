{
  "name": "pgmock2",
  "version": "2.0.0-beta.5",
  "description": "An NPM module for mocking a connection to a PostgreSQL database.",
  "homepage": "https://jfavrod.github.io/pgmock2/",
  "main": "lib/index.js",
  "scripts": {
    "build": "run-s lint test compile",
    "clean": "rm -rf lib",
    "clean-compile": "run-s clean compile",
    "compile": "tsc -p tsconfig.json --incremental",
    "docs": "typedoc src",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "npm:publish": "npm publish",
    "pub": "run-s lint test clean-compile docs npm:publish",
    "test": "./node_modules/.bin/mocha -r ts-node/register test/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jfavrod/pgmock2.git"
  },
  "keywords": [
    "pg",
    "postgres",
    "postgresql",
    "mock",
    "test",
    "testing"
  ],
  "author": "Jason Favrod <mail@jasonfavrod.com> (http://jasonfavrod.com)",
  "license": "MIT",
  "devDependencies": {
    "@types/md5": "^2.1.33",
    "@types/mocha": "^5.2.7",
    "@types/pg": "^7.11.1",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typedoc": "^0.15.5",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "md5": "^2.2.1"
  },
  "engines": {
    "node": "^10.15.0",
    "npm": "^6.13.0"
  }
}
