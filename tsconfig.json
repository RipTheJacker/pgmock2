{
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": true,
    "incremental": true,
    "module": "commonjs",
    "noImplicitAny": false,
    "outDir": "lib",
    "rootDir": "./src",
    "strict": true,
    "target": "es5"
  },
  "exclude": [
    "docs",
    "lib",
    "test"
  ],
  "typedocOptions": {
    "mode": "file",
    "out": "docs"
  }
}
